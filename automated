// this used for delete all video on playlist
// from this guy github.com/timothyarmstrong
(function() {
  var i = window.setInterval(function() {
    var closeButton = document.querySelector('.pl-video-edit-remove');
    if (closeButton) {
      closeButton.click();
    } else {
      window.clearInterval(i);
    }
  }, 500);
})();

