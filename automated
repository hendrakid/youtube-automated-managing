$(function() {
var x=100;//video count
  for(var i=0; i<c; i++;){
      var e = $.Event('keypress');
      e.which = 32; // Space
      $('item').trigger(e);
      var x = $.Event('keypress');
      x.which = 40; // arow key down
      $('item').trigger(x);
  }
});
